{
  "locales": ["en", "fr", "en-FR"],
  "sourceLocale": "en-FR",
  "fallbackLocales": {
    "default":"en"
  },
  "catalogs": [
    {
      "path": "./i18n/lingui/locales/{locale}/messages",
      "include": "src"
    }
  ],
  "format": "po",
  "extractBabelOptions": {
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ],
    "presets": [
      "@babel/preset-env",
      "babel-preset-gatsby",
      "@babel/preset-react",
      "@lingui/babel-preset-react"
    ]
  }
 }