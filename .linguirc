{
  "locales": ["en", "fr", "en-FR"],
  "sourceLocale": "en-FR",
  "fallbackLocales": {
    "default":"en"
  },
  "pseudoLocale": '',
  "catalogs": [
    {
      "path": "./i18n/lingui/locales/{locale}/messages",
      "include": "src"
    }
  ],
  "format": "po",
    "extractBabelOptions": {
    "presets": ["babel-preset-gatsby"]
    }
 }